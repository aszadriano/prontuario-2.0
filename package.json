{
  "name": "prontuario-2.0",
  "version": "2.0.0",
  "description": "Sistema de Prontuário Médico Eletrônico - Versão 2.0 Completa",
  "type": "module",
  "workspaces": [
    "apps/api",
    "packages/shared"
  ],
  "scripts": {
    "dev": "vite",
    "build:front": "tsc && vite build",
    "build:shared": "npm run build --workspace @prontuario/shared",
    "build:api": "npm run build --workspace api",
    "build": "npm run build:shared && npm run build:api && npm run build:front",
    "preview": "vite preview",
    "start": "node server.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.0",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
